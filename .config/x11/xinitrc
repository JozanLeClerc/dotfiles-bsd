# vim: filetype=sh

startif() {
	pgrep "$1" >/dev/null || "$@" >/dev/null 2>&1 &
}

"$HOME"/.local/bin/setwp
"$HOME"/.local/bin/kb
xset s off
startif dunst
startif picom
pgrep musicpd >/dev/null || musicpd >/dev/null 2>&1
startif mpd-notification
startif lowbat
cd "$HOME"
xrdb "$HOME"/.config/x11/xresources
notify-send -u normal 'Welcome' "ïŒŒ  Welcome back, partner\!"
dbus-launch dwm
