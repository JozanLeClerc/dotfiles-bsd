			</action>
		</item>
		<item label="st">
			<action name="Execute">
				<command>st</command>
			</action>
		</item>
		<item label="PCMan File Manager">
			<action name="Execute">
				<command>pcmanfm</command>
			</action>
		</item>
		<item label="Qalculate!">
			<action name="Execute">
				<command>qalculate-gtk</command>
			</action>
		</item>
		<item label="Kleopatra">
			<action name="Execute">
				<command>kleopatra</command>
			</action>
		</item>
		<item label="Transmission">
