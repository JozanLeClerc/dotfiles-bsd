			</action>
		</item>
		<item label="Liferea">
			<action name="Execute">
				<command>liferea</command>
			</action>
		</item>
		<item label="HexChat">
			<action name="Execute">
				<command>hexchat</command>
			</action>
		</item>
	</menu>

	<menu id="apps-audio-menu" label="Audio">
