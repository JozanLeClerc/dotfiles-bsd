#!/bin/sh

echo "devfs /usr/loacal/jails/devuan/dev devfs rw 0 0" >> /etc/fstab
echo "fdescfs /usr/loacal/jails/devuan/dev/fd fdescfs rw,linrdlnk 0 1" >> /etc/fstab
echo "linsys /usr/loacal/jails/devuan/sys linsysfs rw 0 0" >> /etc/fstab
echo "linproc /usr/loacal/jails/devuan/proc linprocfs rw 0 0" >> /etc/fstab

mount /usr/loacal/jails/devuan/dev
mount /usr/loacal/jails/devuan/dev/fd
mount /usr/loacal/jails/devuan/proc
mount /usr/loacal/jails/devuan/sys

sysctl compat.linux.osrelease=4.19.0
kldload linux
kldload linux64

chroot /compat/devuan /bin/bash
