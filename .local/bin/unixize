#!/usr/local/bin/perl

use warnings;
use strict;
use File::Copy;

sub get_og_files
{
	my @og_files;
	my $file;

	opendir(DIR, '.') or die $!;
	while ($file = readdir(DIR)) {
		next if ($file =~ m/^\./);
		push @og_files, $file;
	}
	closedir(DIR);
	return @og_files;
}

sub get_new_files
{
	my @og_files = @_;
	my @new_files;

	push @new_files, @og_files;
	foreach (@new_files) {
	}
	return @new_files;
}

sub main
{
	my @og_files;
	my @new_files;

	@og_files = get_og_files();
	@new_files = get_new_files(@og_files);
	foreach (@new_files) {
		print "'$_'\n";
	}
	return;
}

main();

__END__
